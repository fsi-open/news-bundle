{
    "name": "fsi/news-bundle",
    "type": "symfony-bundle",
    "description": "Provides news for you Symfony2 application.",
    "keywords": ["news", "symfony2", "bundle", "fsi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3",
        "white-october/pagerfanta-bundle": "1.0.*"
    },
    "require-dev": {
        "doctrine/orm": "~2.3",
        "doctrine/doctrine-bundle": "dev-master",
        "symfony/form" : "~2.3",
        "sensio/framework-extra-bundle": "~2.3",
        "fzaninotto/faker": "v1.3.0",
        "phpspec/phpspec": "2.0.*@dev",
        "behat/behat": "2.4.*@stable",
        "behat/symfony2-extension": "*@stable",
        "behat/mink-extension": "*",
        "behat/mink-browserkit-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "sensiolabs/behat-page-object-extension": "1.0.*@dev",
        "bossa/phpspec2-expect": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "FSi\\Bundle\\NewsBundle\\": ""
        }
    },
    "target-dir" : "FSi/Bundle/NewsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
